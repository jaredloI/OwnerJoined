function announce(title,text,time)
    game.StarterGui:SetCore("SendNotification", {
        Title = title;
        Text = text;
        Duration = time;
    })
end


local usernames = {"HAHATDOGKA1011", "Eymb0tsu"}
game.Players.PlayerAdded:Connect(function(plr)
    for i, v in pairs(usernames) do
        if plr.Name == v then
        announce('Redware - Owner', 'Redware Owner Jared is in the Game!', 10)

      end
    end
end)
